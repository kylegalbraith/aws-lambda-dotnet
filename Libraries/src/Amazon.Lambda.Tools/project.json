{
  "name": "Amazon.Lambda.Tools",
  "title": "Amazon.Lambda.Tools",
  "description": "Amazon.Lambda.Tools adds commands to the dotnet cli to deploy AWS Lambda functions.",
  "authors": [
    "Amazon Web Services"
  ],
  "version": "1.1.0-preview1",

  "buildOptions": {
    "outputName": "dotnet-lambda",
    "emitEntryPoint": true,
    "warningsAsErrors": true
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },

    "System.IO.Compression.ZipFile": "4.0.1",
    "System.Runtime.Loader": "4.0.0",

    "AWSSDK.CloudFormation": "[3.3.3, 3.4)",
    "AWSSDK.Lambda": "[3.3.2, 3.4)",
    "AWSSDK.IdentityManagement": "[3.3.0.3, 3.4)",
    "AWSSDK.S3": "[3.3.4, 3.4)"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50",
      "buildOptions": {
        "define": [
          "NETCORE"
        ]
      }
    }
  },
  "packOptions": {
    "licenseUrl": "http://aws.amazon.com/apache2.0/",
    "projectUrl": "https://aws.amazon.com/lambda/",
    "iconUrl": "http://media.amazonwebservices.com/aws_singlebox_01.png",
    "tags": [ "AWS", "Amazon", "Lambda" ]
  }
}
