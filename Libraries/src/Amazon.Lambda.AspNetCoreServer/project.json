{
  "name": "Amazon.Lambda.AspNetCoreServer",
  "title": "Amazon.Lambda.AspNetCoreServer",
  "description": "Amazon.Lambda.AspNetCoreServer makes it easy to run ASP.NET Core Web API applications as AWS Lambda functions.",
  "authors": [
    "Amazon Web Services"
  ],
  "version": "0.8.6-preview1",

  "buildOptions": {
    "xmlDoc": true
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Microsoft.AspNetCore.Hosting": "1.0.0",
    "Newtonsoft.Json": "9.0.1",
    "System.Text.Encodings.Web": "4.0.0",

    "Amazon.Lambda.Core": "[1.0.0, 1.1)",
    "Amazon.Lambda.Serialization.Json": "[1.0.1, 1.1)",
    "Amazon.Lambda.APIGatewayEvents": "[1.0.2, 1.1)"
  },

  "frameworks": {
    "netstandard1.3": {
      "imports": "dnxcore50"
    }
  },
  "packOptions": {
    "licenseUrl": "http://aws.amazon.com/apache2.0/",
    "projectUrl": "https://aws.amazon.com/lambda/",
    "iconUrl": "http://media.amazonwebservices.com/aws_singlebox_01.png",
    "tags": [ "AWS", "Amazon", "Lambda", "aspnetcore" ]
  }
}
